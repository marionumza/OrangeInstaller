# encoding: utf-8       
from OpenOrange import *

ParentFreightCompanyWindow = SuperClass("FreightCompanyWindow","AddressableWindow",__file__)
class FreightCompanyWindow(ParentFreightCompanyWindow):
             
        
    def afterEdit(self, fieldname):
        ParentFreightCompanyWindow.afterEdit(self, fieldname)
        freightcny = self.getRecord()
        freightcny.showMessages()
        if (fieldname == "Contact"):
            freightcny.pasteContact()


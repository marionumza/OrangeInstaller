#encoding: utf-8
from OpenOrange import *
from Report import Report

ParentClassificationWindow = SuperClass("ClassificationWindow", "ClassifierWindow", __file__)
class ClassificationWindow(ParentClassificationWindow):

    def showItems(self):
        from ItemList import ItemList
        report = ItemList()
        report.defaults()
        report.getRecord().Classification = self.getRecord().Code
        report.open(False)

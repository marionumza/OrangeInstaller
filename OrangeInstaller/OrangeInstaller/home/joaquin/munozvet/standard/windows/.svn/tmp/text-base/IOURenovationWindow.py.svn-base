from OpenOrange import *

ParentIOURenovationWindow = SuperClass('IOURenovationWindow', "FinancialTransWindow", __file__)
class IOURenovationWindow(ParentIOURenovationWindow): 

    def afterEdit(self, fieldname):
        ParentIOURenovationWindow.afterEdit(self, fieldname)
        gr = self.getRecord()
        if (fieldname == "OldCheque"):
            gr.pasteOldCheque()
        elif (fieldname == "NewCheque"):
            gr.pasteNewCheque()
        elif (fieldname == "CustCode"):
            gr.pasteCustCode()

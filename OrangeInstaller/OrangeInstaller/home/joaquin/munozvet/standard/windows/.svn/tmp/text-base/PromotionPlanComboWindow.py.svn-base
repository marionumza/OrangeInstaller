#encoding: utf-8
from OpenOrange import *

ParentWindow = SuperClass("PromotionPlanComboWindow","Window",__file__)
class PromotionPlanComboWindow(ParentWindow):

    def afterEditRow(self, fieldname, rowfieldname, rownr):
        ParentWindow.afterEditRow(self, fieldname, rowfieldname, rownr)
        record = self.getRecord()
        if fieldname == "Items":
            if rowfieldname == "ArtCode":
                record.Items[rownr].pasteArtCode()
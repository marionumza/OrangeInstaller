from OpenOrange import *

ParentConversationMessagePostWindow = SuperClass('ConversationMessagePostWindow', "Window", __file__)
class ConversationMessagePostWindow(ParentConversationMessagePostWindow): 

    def buttonClicked(self, buttonname):
        ParentConversationMessagePostWindow.buttonClicked(self, buttonname)
        if buttonname == "send":
            self.getRecord().send()
            self.Origin.refresh()
            self.close()
              
    def save_fromC(self):
        ParentConversationMessagePostWindow.save_fromC(self)
        alert("a")

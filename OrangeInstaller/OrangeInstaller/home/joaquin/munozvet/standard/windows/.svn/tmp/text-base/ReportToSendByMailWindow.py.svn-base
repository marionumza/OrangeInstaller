#encoding: utf-8
from OpenOrange import *

ParentReportToSendByMailWindow = SuperClass("ReportToSendByMail","Window",__file__)
class ReportToSendByMailWindow(ParentReportToSendByMailWindow):

    def fillPasteWindow(self, pastewindowname, fieldname):
        res = ParentReportToSendByMailWindow.fillPasteWindow(self, pastewindowname, fieldname)
        if pastewindowname == "XPasteWindow" and fieldname == "Report":
            return self.getRecord().getReportList()
        return res

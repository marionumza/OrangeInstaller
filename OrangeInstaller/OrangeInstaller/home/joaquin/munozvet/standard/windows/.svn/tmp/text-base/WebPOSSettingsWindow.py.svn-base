#encoding: utf-8
from OpenOrange import *

ParentWebPOSSettingsWindow = SuperClass("WebPOSSettingsWindow","SettingWindow",__file__)
class WebPOSSettingsWindow(ParentWebPOSSettingsWindow):
        
    def afterEditRow(self, fieldname, rowfieldname, rownr):
        ParentWebPOSSettingsWindow.afterEditRow(self,fieldname, rowfieldname,rownr)
        if fieldname == "PayModes":
          if rowfieldname == "PayModeCode":
              self.getRecord().PayModes[rownr].pastePayModeCode()

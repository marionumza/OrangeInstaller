#encoding: utf-8
from OpenOrange import *

ParentAccountSettingsWindow = SuperClass("AccountSettingsWindow","SettingWindow",__file__)
class AccountSettingsWindow(ParentAccountSettingsWindow):


    def afterEdit(self, fieldname):
        ParentAccountSettingsWindow.afterEdit(self,fieldname)
        record = self.getRecord()
        if (fieldname == "RoundingAcc"):
            record.RoundGainAcc = record.RoundingAcc
            record.RoundLossAcc = record.RoundingAcc

          